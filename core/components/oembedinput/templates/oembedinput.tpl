<div class="contentblocks-loader"></div>
<div class="contentblocks-field contentblocks-field-oembed">
    <div class="contentblocks-field-actions">
        <a href="javascript:void(0);" class="contentblocks-field-unset-embed">&times; {%=_('oembedinput.unset_embed')%}</a>
    </div>
    <label for="{%=o.generated_id%}_textfield">{%=o.name%}</label>
    <div class="contentblocks-field-text paste_link">
        <input type="text" class="link" placeholder="{%=_('contentblocks.video.paste_link')%}">
    </div>
    <div class="meta">
        <p>
            <span class="provider_name"></span>
            <a href="#" class="title"></a>
        </p>
        <p class="url"></p>
    </div>
    <div class="embed embeds-behave"></div>
</div>
